<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    >
	<SelectDialog
		id="valueBookHelpDialog"
		title="Books"
		items="{LibraryData>/books}"
		search="_handleValueBookHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose"
        contentWidth="500px"
        contentHeight="500px"
		>
		<StandardListItem
			title="{LibraryData>TITLE}"
			description="{LibraryData>ISBN}" />
	</SelectDialog>
</core:FragmentDefinition>