<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueCustHelpDialog"
		title="Customers"
		items="{LibraryData>/customers}"
		search="_handleCustValueHelpSearch"
		confirm="_handleCustValueHelpClose"
		cancel="_handleCustValueHelpClose"
        contentWidth="500px"
        contentHeight="500px"
		>
		<StandardListItem id="CustList"
			title="{LibraryData>NAME}"
			description="{LibraryData>EMAIL}" />
	</SelectDialog>
</core:FragmentDefinition>